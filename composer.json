{
    "name": "opanegro/dot-nova-sidebar-collapse",
    "description": "Create custom menu resources with category collapse and custom icon",
    "keywords": [
        "laravel",
        "nova",
        "collapse",
        "sidebar"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Opanegro\\DotNovaSidebarCollapse\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Opanegro\\DotNovaSidebarCollapse\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
